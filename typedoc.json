{
  "entryPoints": ["src/docs.ts"],
  "out": "docs-build",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "hideGenerator": true,
  "theme": "default",
  "includeVersion": true,

  "plugin": ["typedoc-plugin-coverage"],

  "coverageLabel": "docs",
  "coverageSvgWidth": 80
}
